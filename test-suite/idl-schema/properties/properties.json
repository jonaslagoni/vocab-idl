[
  {
    "description": "Should have accurate property types",
    "schemas": [
      {
        "$schema": "https://json-schema.org/draft/2020-12/idl-schema",
        "name": "SomeTitle",
        "type": "object",
        "properties": {
          "StringProperty": {
            "type": "string"
          },
          "BooleanProperty": {
            "type": "boolean"
          },
          "NumberProperty": {
            "type": "number"
          },
          "NullProperty": {
            "type": "null"
          }
        }
      }
    ],
    "tests": [
      {
        "assertion": "hasClass",
        "tests": [
          {
            "assertion": "hasName",
            "arguments": [
              "SomeTitle"
            ]
          },
          {
            "assertion": "hasProperty",
            "tests": [
              {
                "assertion": "hasName",
                "arguments": [
                  "StringProperty"
                ]
              },
              {
                "assertion": "hasType",
                "arguments": [
                  "string"
                ]
              }
            ]
          },
          {
            "assertion": "hasProperty",
            "tests": [
              {
                "assertion": "hasName",
                "arguments": [
                  "BooleanProperty"
                ]
              },
              {
                "assertion": "hasType",
                "arguments": [
                  "boolean"
                ]
              }
            ]
          },
          {
            "assertion": "hasProperty",
            "tests": [
              {
                "assertion": "hasName",
                "arguments": [
                  "NumberProperty"
                ]
              },
              {
                "assertion": "hasType",
                "arguments": [
                  "number"
                ]
              }
            ]
          }
        ]
      }
    ]
  }
]