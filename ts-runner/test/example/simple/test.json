              ]
            },
            {
              "assertion": "hasProperty",
              "tests": [
                {
                  "assertion": "hasName",
                  "arguments": [
                    "aaa"
                  ]
                },
                {
                  "assertion": "hasType",
                  "arguments": [
                    "string",
                    {
                      "language": "java",
                      "value": "String"
                    }
                  ]
                }
              ]
            },
            {
              "assertion": "hasProperty",
              "tests": [
                {
                  "assertion": "hasName",
                  "arguments": [
                    "bbb"
                  ]
                },
                {
                  "assertion": "hasType",
                  "arguments": [
                    "boolean",
                    {
                      "language": "java",
                      "value": "Boolean"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
